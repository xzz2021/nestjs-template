{
  "name": "backxzz",
  "version": "0.0.1",
  "description": "架构模板",
  "author": "xzz2021",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production nest build",
    "build:docker": "cross-env NODE_ENV=docker nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production  node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "dev": "pnpm lint && cross-env NODE_ENV=development nest start --watch",
    "prisma:g:pg": "npx prisma generate --schema src/prisma/pg.schema.prisma",
    "prisma:g:mysql": "npx prisma generate --schema src/prisma/mysql.schema.prisma",
    "prisma:push:pg": "npx prisma db push --schema src/prisma/pg.schema.prisma",
    "prisma:push:mysql": "npx prisma db push --schema src/prisma/mysql.schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --schema src/prisma/schema.prisma",
    "prisma:seed": "npx prisma db seed",
    "prisma:pull": "npx prisma db pull --schema src/prisma/pg.schema.prisma",
    "prisma:format": "npx prisma format --schema src/prisma/pg.schema.prisma",
    "prepare": "husky",
    "ts:check": "tsc --noEmit --skipLibCheck",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.cjs"
  },
  "prisma": {
    "seed": "ts-node src/prisma/seed.ts"
  },

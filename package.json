{
  "name": "backxzz",
  "version": "0.1.0",
  "description": "架构模板",
  "author": "xzz2021",
  "scripts": {
    "build": "cross-env NODE_ENV=production nest build",
    "build:minify": "cross-env NODE_ENV=production nest build && node scripts/minify-build.js",
    "build:docker": "cross-env NODE_ENV=docker nest build",
    "build:docker:minify": "cross-env NODE_ENV=docker nest build && node scripts/minify-build.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production  node dist/main",
    "start:docker": "cross-env NODE_ENV=docker  node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "dev": "pnpm lint && cross-env NODE_ENV=development nest start --watch",
    "prisma:deploy": "npx prisma migrate deploy --schema src/prisma/schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --name  init",
    "prisma:seed": "npx prisma db seed",
    "prisma:pull": "npx prisma db pull",
    "prisma:format": "npx prisma format",
    "prisma:diysql": "npx prisma migrate dev --create-only --name ",
    "prisma:diysql2": "create-only会创建空的草稿sql,编辑后,再次执行npx prisma migrate dev执行迁移",
    "prepare": "husky",
    "ts:check": "tsc --noEmit --skipLibCheck",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.cjs"
  },
